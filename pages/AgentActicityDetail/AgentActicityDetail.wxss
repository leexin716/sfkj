/* pages/AgentActicityDetail/AgentActicityDetail.wxss */
page{
  background: #80afff
}
.contain{
  position: relative
}
.count{
  color: #fff;
  position: absolute;
  top:740rpx;
  width: 100%;
  text-align: center
}
.count text{
  font-size: 48rpx;
  color: #FFD222;
  font-weight: bold
}
.rankContent{
  background: #4f4cff;
  width: 90%;
  padding: 100rpx 0;
  border-radius: 30rpx;
  position: relative;
  margin: auto;
}
.rankContent image{
  position: absolute;
  top: -70rpx;
  left: 40rpx;
}
.rankFont{
  color: #fff;
  position: absolute;
  top:-28rpx;
  width: 100%;
  text-align: center;
  font-size: 32rpx;
}
.rankList{
  color: #fff;
  font-size: 28rpx;
  width: 90%;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}
.rankListRight{
  width: 90%;
  text-align: left;
  display: flex;
  align-items: center;
  justify-content: space-around
}
.rankListLeft{
  width: 40rpx;
  height: 40rpx;
  border:1px solid #fff;
  border-radius: 50%;
  text-align: center
}
.lookBtn{
  width: 80%;
  line-height: 80rpx;
  color: #fff;
  font-size: 32rpx;
  margin: auto;
  background: #ff8546;
  text-align: center;
  margin-top: 30rpx;
  border-radius: 60rpx;
}