/* 二维码生成器 */

.mask {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 3;
  width: 100%;
  height: 100%;
  opacity: 0;
}

.canvas-box {
  position: fixed;
  top: 999999rpx;
  left: 0;
}

.img-box {
  width: 156rpx;
  height: 156rpx;
  /* border-radius: 50%; */
  background-size: 100% 100%;
}

.img-box image {
  width: 156rpx;
  height: 156rpx;
  background-size: 100% 100%;
}

